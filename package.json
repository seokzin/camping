    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.11.8",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "typescript": "^4.8.4"
  },
  "packageManager": "yarn@3.2.4"
}
